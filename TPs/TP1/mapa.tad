		// esVacio(agregarPared(p,t)) ≡ false
		
		inicio(tableroVacio(h,w,i,l)) ≡ i
		final(tableroVacio(h,w,i,l)) ≡ l
		alto(tableroVacio(h,w,i,l)) ≡ h
		ancho(tableroVacio(h,w,i,l)) ≡ w

		
		inicio(agregarFantasma(p, t)) ≡ inicio(t)
		inicio(agregarPared(p, t)) ≡ inicio(t)
		
		
		fantasmas(tableroVacio(h,w,i,l)) ≡ Ø
		fantasmas(agregarFantasma(p2,t)) ≡ p ∪ fantasmas(t)
		t = agregarFantasma(p1, tableroVacio)
		t = tableroVacio
		

		
		
