TAD Tablero 
	Igualdad Observacional:
	Generos: tablero
	Exporta: tablero, observadores, generadores, posicion, conj(α)
	Usa: Bool, Nat, posicion
	
	Observadores Basicos:
		// esVacio: tablero -> bool
		inicio: tablero -> posicion
		final: tablero -> posicion
		fantasmas: tablero -> conj(posicion)
		// pacman: tablero -> posicion
		paredes: tablero -> conj(posicion)
		alto: tablero -> nat
		ancho: tablero -> nat
		
	Generadores:
	
		// nuevoPacman:	inicio -> tablero
		
		// tableroVacio: -> tablero
		// agregarFila: nat -> tablero
		
		//PREGUNTAR SI TABLERO VACIO ESTÁ BIEN.
		tableroVacio: nat a × nat b × posicion i × posicion f -> tablero  {¬(i = f) ∧ πi) <= a ∧ π2(f) <= b)(tablero) < b y }
		agregarFantasma: posicion p × tablero t -> tablero
												{posicionValida ∧ ¬esPared(p, t) ∧ ¬esFantasma(p, t) ∧ ¬(p = inicio(t))
												∧ ¬(p = final(t))}
		agregarPared: posicion × tablero -> tablero 
		                    {posicionValida ∧ ¬esPared(p, t) ∧ ¬esFantasma(p, t) ∧ ¬(p = inicio(t))
												∧ ¬(p = final(t))}
		
		
		
		// agregarPacman:
		
	Otras Operaciones:
		posicionValida: posicion × tablero -> bool
		esPared: posicion × tablero -> bool
		esFantasma: posicion × tablero -> bool
		
